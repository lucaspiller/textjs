<% threads.each(function(thread) { %>
  <div class="thread" data-id="<%= thread.id %>">
    <header>
      <span class="contact">
        <%= thread.contactName() %>
      </span>
      <span class="date">
        <%= thread.date() %>
      </span>
    </header>
    <p>
      <%= thread.get('body') %>
    </p>
  </div>
<% }); %>
