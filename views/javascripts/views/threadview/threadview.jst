<table>
  <% messages.each(function(message) { %>
    <tr class="summary" data-id="<%= message.get('id') %>">
      <% if (message.get('type') == message.TYPE_INBOX) { %>
        <td class="contact"><%= message.contactName() %></td>
      <% } else { %>
        <td class="contact">You</td>
      <% } %>
      <td><%= message.get('body') %></td>
      <td><%= message.date() %></td>
    </div>
  <% }); %>
</table>
